# SOPHIA Intel Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit actual secrets to version control

# Core API Keys
OPENROUTER_API_KEY=your_openrouter_api_key_here
GITHUB_PAT=your_github_personal_access_token_here
DNSIMPLE_API_KEY=your_dnsimple_api_key_here

# Database URLs (provided by Railway services)
QDRANT_URL=${Qdrant.QDRANT_URL}
REDIS_URL=${Redis.REDIS_URL}
NEON_DATABASE_URL=${NeonDB.DATABASE_URL}

# Railway Configuration
RAILWAY_TOKEN=your_railway_token_here

# Application Configuration
NODE_ENV=production
PYTHON_ENV=production
LOG_LEVEL=info

# Health Check Configuration
HEALTH_CHECK_TIMEOUT=30
HEALTH_CHECK_INTERVAL=60

# Security Configuration
CORS_ORIGINS=https://www.sophia-intel.ai,https://api.sophia-intel.ai
ALLOWED_HOSTS=www.sophia-intel.ai,api.sophia-intel.ai

# Monitoring and Observability
SENTRY_DSN=your_sentry_dsn_here
PROMETHEUS_ENABLED=true
METRICS_PORT=9090

# Feature Flags
ENABLE_CHAT=true
ENABLE_RESEARCH=true
ENABLE_CODING=true
ENABLE_MCP_SERVICES=true
