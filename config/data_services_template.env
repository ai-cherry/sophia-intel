# Data Services Configuration Template
# Copy these to your .env file and fill in the actual values

# Redis Cache (Redis Cloud)
# Sign up at: https://redis.com/try-free/
# Format: redis://default:PASSWORD@ENDPOINT:PORT
REDIS_URL=redis://default:YOUR_PASSWORD@YOUR_ENDPOINT.redislabs.com:12345

# PostgreSQL Database (Neon)
# Sign up at: https://neon.tech/
# Format: postgresql://username:password@host/database?sslmode=require
DATABASE_URL=postgresql://username:password@ep-example.us-east-1.aws.neon.tech/sophia_intel?sslmode=require

# Qdrant Vector Database
# Get API key from: https://cloud.qdrant.io/
# Note: Use the API key, not the cluster ID
QDRANT_API_KEY=qk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
QDRANT_URL=https://your-cluster-id.us-east4-0.gcp.cloud.qdrant.io:6333

# Estuary Flow Data Pipeline
# Get token from: https://dashboard.estuary.dev/
# API Base URL appears to be: https://dashboard.estuary.dev/api
ESTUARY_ACCESS_TOKEN=your_access_token_here
ESTUARY_API_BASE=https://dashboard.estuary.dev/api

# Additional Redis Configuration (if using different provider)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# Additional PostgreSQL Configuration (if using different provider)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=sophia_intel
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password

