# SOPHIA Intel Environment Variables Template
# Copy to .env and fill in your actual values

# GitHub Integration
GITHUB_PAT=your_github_personal_access_token_here
GITHUB_REPO=ai-cherry/sophia-intel

# OpenRouter AI Models
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Lambda Labs Infrastructure
LAMBDA_API_KEY=your_lambda_labs_api_key_here
LAMBDA_CLOUD_API_KEY=your_lambda_cloud_api_key_here

# DNSimple DNS Management
DNSIMPLE_API_KEY=your_dnsimple_api_key_here

# Vector Database
QDRANT_URL=your_qdrant_cluster_url_here
QDRANT_API_KEY=your_qdrant_api_key_here

# Data Pipeline
AIRBYTE_URL=your_airbyte_instance_url_here
AIRBYTE_TOKEN=your_airbyte_api_token_here

# Production Configuration
PRODUCTION_DOMAIN=sophia-intel.ai
PRODUCTION_IP=your_production_server_ip_here
